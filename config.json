  "early_stop": true,
  "patience": 20,
  "resume": true,
  "resume_path": "./saved_models/model_best.pt",
  "log_step": 20,
  "max_grad_norm": 5.0,
  "cuda": true,
  "config_file": "./config.json",
  "seed": 1234
}