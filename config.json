{
  "train_data": "datasets/train.json",
  "valid_data": "datasets/test.json",
  "vocab": "datasets/vocab.pkl",
  "embed_matrix": "datasets/embedding.npy",
  "lower": true,
  "batch_size": 50,
  "word_dropout": 0.04,
  "h_dim": 360,
  "emb_dim": 300,
  "pos_dim": 30,
  "n_mlp_layers": 1,
  "n_blocks": 2,
  "n_heads": 3,
  "topn": 10000000000.0,
  "rnn": true,
  "rnn_dim": 300,
  "n_rnn_layers": 1,
  "rnn_dropout": 0.5,
  "n_sub_layers_first": 2,
  "n_sub_layers_second": 4,
  "dropout": 0.3,
  "emb_dropout": 0.3,
  "self_loop": true,
  "pooling": "max",
  "lr": 0.5,
  "optimizer": "sgd",
  "conv_l2": 0,
  "pooling_l2": 0.002,
  "lr_decay": 0.9,
  "lr_decay_patience": 6,
  "epochs": 150,
  "save_dir": "./saved_models",
